#include <iostream>
using namespace std;

int main(void)
{
	FILE* pf;
	freopen_s(&pf, "input.txt", "r", stdin);
	int T;

	cin >> T;

	for (int t = 1; t <= T; t++)
	{
		int N;
		int* arr;
		int sum = 0;

		cin >> N;

		arr = new int[N];

		for (int n = 0; n < N; n++)
		{
			cin >> arr[n];

			sum += arr[n];
		}

		double avg = sum / N;

		int cnt = 0;

		for (int i = 0; i < N; i++)
			if (avg >= (double)arr[i])
				cnt++;

		cout << "#" << t << " " << cnt << endl;
	}

	return 0;
}

