#include <iostream>
using namespace std;

int main(void)
{
	cin.tie(0), cout.tie(0);
	ios::sync_with_stdio(false);
	int T;

	cin >> T;

	for (int t = 1; t <= T; t++)
	{
		int p, q, r;
		int a, b, c, d;

		cin >> p >> q >> r;
		cin >> a >> b >> c >> d;

		int cnt = 0;

		if ((a - p) * (a - p) + (b - q) * (b - q) <= r * r)
			cnt++;

		if ((a - p) * (a - p) + (d - q) * (d - q) <= r * r)
			cnt++;

		if ((c - p) * (c - p) + (b - q) * (b - q) <= r * r)
			cnt++;

		if ((c - p) * (c - p) + (d - q) * (d - q) <= r * r)
			cnt++;

		cout << "#" << t << " ";

		if (cnt == 4)
			cout << "YN\n";
		else if (a <= p - r && c >= p + r && b <= q - r && d >= q + r)
			cout << "NY\n";
		else
			cout << "YY\n";
	}

	return 0;
}
