#include <stdio.h>

int main(void)
{
	FILE* pf;
	freopen_s(&pf, "input.txt", "r", stdin);
	int T;

	scanf_s("%d", &T);

	for (int t = 1; t <= T; t++)
	{
		int p, q, r;
		int a, b, c, d;

		scanf_s("%d %d %d", &p, &q, &r);
		scanf_s("%d %d %d %d", &a, &b, &c, &d);

		int cnt = 0;

		if ((a - p) * (a - p) + (b - q) * (b - q) <= r * r)
			cnt++;

		if ((a - p) * (a - p) + (d - q) * (d - q) <= r * r)
			cnt++;

		if ((c - p) * (c - p) + (b - q) * (b - q) <= r * r)
			cnt++;

		if ((c - p) * (c - p) + (d - q) * (d - q) <= r * r)
			cnt++;

		printf("#%d ", t);

		if (cnt == 4)
			printf("YN\n");
		else if (a <= p - r && c >= p + r && b <= q - r && d >= q + r)
			printf("NY\n");
		else
			printf("YY\n");
	}

	return 0;
}
