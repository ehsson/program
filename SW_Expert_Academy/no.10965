// time limit code

#include <iostream>
using namespace std;

int arr[10001];

int main(void)
{
	FILE* pf;
	freopen_s(&pf, "input.txt", "r", stdin);
	cin.tie(0), cout.tie(0);
	ios::sync_with_stdio(false);
	int T;

	cin >> T;

	for (int i = 1; i <= 10000; i++)
	{
		arr[i] = i * i;
	}

	for (int t = 1; t <= T; t++)
	{
		int A;

		cin >> A;

		int answer = 1;

		for (int i = 1; i <= 10000; i++)
		{
			if (arr[i] >= A)
			{
				if ((double)((arr[i] / A) * A) == (double)arr[i])
				{
					answer = (int)(arr[i] / A);
					break;
				}
			}
		}

		cout << "#" << t << " " << answer << endl;
	}

	return 0;
}
