#include <stdio.h>

int main(void)
{
	int T, input, date, month, year;
	
	scanf("%d", &T);
	
	for(int t = 0; t < T; t++) {
		scanf("%d", &input);
		
		year = (input / 100) / 100;
		
		month = (input / 100) % 100;
		
		if(month > 12 || month < 1)
			printf("#%d -1\n", t + 1);
		else {
			date = input % 100;
			
			if(month == 1 || month == 3 || month == 5 || month == 7 || month == 8 || month == 10 || month == 12) {
				if(date > 31 || date < 1)
					printf("#%d -1\n", t + 1);
				else
					printf("#%d %04d/%02d/%02d\n", t + 1, year, month, date);
			}
			else if(month == 2) {
				if(date > 28 || date < 1)
					printf("#%d -1\n", t + 1);
				else
					printf("#%d %04d/%02d/%02d\n", t + 1, year, month, date);
			}
			else {
				if(date > 30 || date < 1)
					printf("#%d -1\n", t + 1);
				else
					printf("#%d %04d/%02d/%02d\n", t + 1, year, month, date);
			}
		}
	}
	
	return 0;
}
