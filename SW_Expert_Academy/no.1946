#include <stdio.h>

int main(void)
{
	int T, N, cnt;
	char alph[20] = { 0 };
	int num[20] = { 0 };
	
	scanf("%d", &T);
	
	for(int t = 1; t <= T; t++) {
		scanf("%d", &N);
		
		cnt = 0;
		
		for(int i = 0; i < N; i++)
			scanf("%c %d", &alph[i], &num[i]);
		
		printf("#%d\n", t);
		
		for(int k = 0; k < N; k++) {
			for(int c = 0; c < num[k]; c++) {
				printf("%c", alph[c]);
				cnt++;
				
				if(cnt % 10 == 0)
					printf("\n");
			}
		}
	}
	
	return 0;
}
