// 아빠 솔루션

#include <stdio.h>

int main(void)
{
	int N, quotient, remainder, chk;

	scanf_s("%d", &N);

	for (int n = 1; n <= N; n++) {
		chk = 0;
		quotient = n;

		while (1) {
			remainder = quotient % 10;
			quotient = (int)(quotient / 10);

			if (remainder == 3 || remainder == 6 || remainder == 9)
				chk++;

			if (quotient == 0)
				break;
		}

		if (chk == 0)
			printf("%d ", n);
		else {
			for (int i = 0; i < chk; i++)
				printf("-");

			printf(" ");
		}
	}

	return 0;
}


