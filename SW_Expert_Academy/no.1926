#include <stdio.h>

int main(void)
{
	int N, cnt;
	
	scanf("%d", &N);
	
	for(int n = 1; n <= N; n++) {
		cnt = 0;
		
		if(n % 10 == 0)
			cnt = cnt;
		else if((n % 10) % 3 == 0) cnt++;
		
		if(n / 10 == 0) 
			cnt = cnt;
		else if((n / 10) % 10 == 0)
			cnt = cnt;
		else if(((n / 10) % 10) % 3 == 0) cnt++;
		
		if(n / 100 == 0) 
			cnt = cnt;
		else if((n / 100) % 3 == 0) cnt++;
		
		switch (cnt) {
			case 0:
				printf("%d ", n);
				break;
			case 1:
				printf("- ");
				break;
			case 2:
				printf("-- ");
				break;
			case 3:
				printf("--- ");
				break;	
		}
	}
	
	return 0;
}
