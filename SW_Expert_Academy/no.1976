//수정 전 -> 틀렸음 : 6 10 6 20,   12 10 12 20  -> 둘 다  0 30 으로 출력

#include <stdio.h>

int main(void)
{
	int T, hour1, hour2, min1, min2, sum_hour, sum_min;
	
	scanf("%d", &T);
	
	for(int t = 0; t < T; t++) {
		scanf("%d %d %d %d", &hour1, &min1, &hour2, &min2);
		
		sum_hour = (hour1 + hour2) % 12;
		sum_min = (min1 + min2) % 60;
		
		if((min1 + min2) / 60 >= 1)
			sum_hour++;
		
		printf("#%d %d %d\n", t + 1, sum_hour, sum_min);
	}
	
	return 0;
}

//수정한 코드 -> 틀렸음 : 12 10 12 20  ->  0 30 으로 출력

#include <stdio.h>

int main(void)
{
	int T, hour1, hour2, min1, min2, sum_hour, sum_min;
	
	scanf("%d", &T);
	
	for(int t = 0; t < T; t++) {
		scanf("%d %d %d %d", &hour1, &min1, &hour2, &min2);
		
		if(hour1 + hour2 == 12) 
			sum_hour = 12;
		else
			sum_hour = (hour1 + hour2) % 12;
		
		sum_min = (min1 + min2) % 60;
		
		if((min1 + min2) / 60 >= 1)
			sum_hour++;
		
		printf("#%d %d %d\n", t + 1, sum_hour, sum_min);
	}
	
	return 0;
}

//아빠 솔루션 1  -> 현수 코드 수정 : 70점

#include <stdio.h>

int main(void)
{
	int T, hour1, hour2, min1, min2, sum_hour, sum_min;
	
	scanf("%d", &T);
	
	for(int t = 0; t < T; t++) {
		scanf("%d %d %d %d", &hour1, &min1, &hour2, &min2);

        sum_min = (min1 + min2) % 60;
        
        if((min1 + min2) / 60 >= 1)
			hour1++;

		sum_hour = (hour1 + hour2) % 12;
        
		if(sum_hour == 0) 
			sum_hour = 12;

		printf("#%d %d %d\n", t + 1, sum_hour, sum_min);
	}
	
	return 0;
}

//아빠 솔루션2 : 100점

#include <stdio.h>

int main(void)
{
	int T, N, sum_hour, sum_min;
	
	scanf("%d", &T);
	
	for(int t = 0; t < T; t++) {
		sum_hour = sum_min = 0;
		
		scanf("%d", &N);
		sum_min = N * 60;

		scanf("%d", &N);
		sum_min += N;

		scanf("%d", &N);
		sum_min += N * 60;

		scanf("%d", &N);
		sum_min += N;

	        sum_hour = ( (int)(sum_min / 60) % 12 );
	        if (sum_hour == 0)
	            sum_hour = 12;
        
	        sum_min = sum_min % 60;
        
		printf("#%d %d %d\n", t + 1, sum_hour, sum_min);
	}
	
	return 0;
}
