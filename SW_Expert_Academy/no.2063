#include <stdio.h>
#define swap(a, b) (a ^= b ^= a ^= b)

void sort(int * arr, int N)
{
	for(int i = 0; i < N - 1; i++) {
		for(int k = i + 1; k < N; k++) {
			if(arr[i] > arr[k]) {
				swap(arr[i], arr[k]);
			}
		}
	}
}

int main(void)
{
	int N;
	int arr[199] = { 0 };
	
	scanf("%d", &N);
	
	for(int i = 0; i < N; i++) 
		scanf("%d", &arr[i]);
	
	sort(arr, N);
	
	printf("%d\n", arr[N / 2]);
	
	return 0;
}
