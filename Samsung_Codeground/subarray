#include <stdio.h>

int arr[100001];

int main(void)
{
	int T, N, S;
	int min, sum;
	int j;
	FILE* pf;
	freopen_s(&pf, "input.txt", "r", stdin);

	scanf_s("%d", &T);

	for (int t = 1; t <= T; t++)
	{
		scanf_s("%d %d", &N, &S);

		min = N + 1;

		for (int n = 1; n <= N; n++)
		{
			scanf_s("%d", &arr[n]);

			if (arr[n] >= S)
				min = 1;
		}

		if (min == 1)
		{
			printf("#testcase%d\n1\n", t);
			continue;
		}

		sum = 0;
		j = 1;

		for (int i = 1; i < N; i++)
		{
			while (sum < S && j <= N)
			{
				sum += arr[j];
				j++;
			}

			if (min > (j - i) && sum >= S)
				min = j - i;

			sum -= arr[i];

			if (min == 2)
				break;
		}

		if (min == N + 1)
			min = 0;

		printf("#testcase%d\n%d\n", t, min);
	}

	return 0;
}

//test case

2
10 15 
5 1 3 5 10 7 4 9 2 8 
5 11
1 2 3 4 5
