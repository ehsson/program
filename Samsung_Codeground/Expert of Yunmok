#include <stdio.h>

int answer[6];

long long GCD(long long a, long long b)
{
	long long temp;

	while (1)
	{
		temp = a % b;
		
		if (temp == 0)
			return b;

		a = b;
		b = temp;
	}
}

void DFS(int idx, int totalsum, int totalcnt)
{
	int cnt = (int)(totalsum / idx);

	while (1)
	{
		int tempsum = totalsum - idx * cnt;
		int tempcnt = totalcnt - cnt;

		if (tempsum - tempcnt > 0)
		{
			answer[idx] = cnt;
			DFS(idx - 1, tempsum, tempcnt);
			return;
		}
		else if (tempsum - tempcnt == 0)
		{
			answer[idx] = cnt;
			answer[1] = tempcnt;
			return;
		}

		cnt--;
	}
}

int main(void)
{
	int T;
	double P;
	FILE* pf;
	freopen_s(&pf, "input.txt", "r", stdin);

	scanf_s("%d", &T);

	for (int t = 1; t <= T; t++)
	{
		scanf_s("%lf", &P);

		for (int k = 1; k <= 5; k++)
			answer[k] = 0;

		long long j = 10e9;
		long long i = P * j;
		long long gcd = GCD(i, j);
		int TotalSum = i / gcd;
		int TotalCnt = TotalSum / P;

		DFS(5, TotalSum, TotalCnt);

		printf("#testcase%d\n%d %d %d %d %d\n", t, answer[1], answer[2], answer[3], answer[4], answer[5]);
	}

	return 0;
}

//test case

3
1.1
4.5
3.20
