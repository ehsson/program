#include <stdio.h>
////////////////////////////////////////////////////////////////////////////////
// N - B = R + TQ를 만족하는
// B = [1 ~ b]
// R = [a ~ ka]
// T = [bk + a ~ ak + b]
// B, R ,T 가 존재하면 무조건 a가 승리하게된다. 
/////////////////////////////////////////////////////////////////////////////
int main(void)
{
    int T, N, K;
    int a, b, c;
    int temp, chk, i;
    char result[8];
    FILE* pf;
    freopen_s(&pf, "input.txt", "r", stdin);

    scanf_s("%d", &T);

    for (int t = 1; t <= T; t++)
    {
        scanf_s("%d %d %d", &a, &b, &c);

        for (i = 0; i < c; i++)
        {
            chk = 0;

            scanf_s("%d %d", &N, &K);

            for (int j = 1; j <= b; j++)
            {
                for (int m = b * K + a; m <= a * K + b; m++)
                {
                    temp = (N - j) % m;

                    if (a <= temp && temp <= K * a)
                    {
                        chk = 1;
                        break;
                    }
                }
                if (chk)
                    break;
            }
            if (chk)
                result[i] = 'a';
            else
                result[i] = 'b';
        }
        result[i] = NULL;

        printf("Case #%d\n", t);

        printf("%s\n", result);

    }

    return 0;
}

// test case

4
1 1 3
10 3
9 3
13 3
2 2 1
10 3
3 2 1
10 3
1 1 1
31 3
