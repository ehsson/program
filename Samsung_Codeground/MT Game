#include <stdio.h>

int main(void)
{
	int T, N, K, a, b, c;
	int chk, temp, tc;
	char arr[7];
	FILE* pf;
	freopen_s(&pf, "input.txt", "r", stdin);

	scanf_s("%d", &T);

	for (int t = 1; t <= T; t++)
	{
		scanf_s("%d %d %d", &a, &b, &c);

		for (tc = 0; tc < c; tc++)
		{
			scanf_s("%d %d", &N, &K);

			chk = 0;

			for (int i = 1; i <= b; i++)
			{
				for (int j = b * K + a; j <= a * K + b; j++)
				{
					temp = (N - i) % j;

					if (a <= temp && temp <= a * K)
					{
						chk = 1;
						break;
					}
				}

				if (chk)
					break;
			}

			if (chk)
				arr[tc] = 'a';
			else
				arr[tc] = 'b';
		}

		arr[tc] = 0;

		printf("Case #%d\n%s\n", t, arr);
	}

	return 0;
}

// test case

4
1 1 3
10 3
9 3
13 3
2 2 1
10 3
3 2 1
10 3
1 1 1
31 3
