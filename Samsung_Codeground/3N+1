#include <stdio.h>
#include <math.h>

int K;
unsigned long long max, min;

void find(long long num, int cnt, int oe)
{
	if (cnt > 0)
		if (num == 1)
			return;

	if (oe == 1)
	{
		if (num % 2 == 0)
			return;
	}

	if (cnt == K)
	{	
		if (min > num)
			min = num;

		return;
	}

	find(num * 2, cnt + 1, 0);

	if (num > 3 && (num - 1) % 3 == 0)
		find(((num - 1) / 3), cnt + 1, 1);
}

int main(void)
{
	int T;
	FILE* pf;
	freopen_s(&pf, "input.txt", "r", stdin);

	scanf_s("%d", &T);

	for (int t = 1; t <= T; t++)
	{
		scanf_s("%d", &K);

		max = pow(2, K);
		min = max;
		
		find(1, 0, 0);

		printf("Case #%d\n%llu %llu\n", t, min, max);
	}

	return 0;
}

// test case

3
3
8
63
