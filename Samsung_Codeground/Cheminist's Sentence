#include <stdio.h>
#include <string.h>

const char chem[][3] = { "H", "He", "Li", "Be", "B", "C", "N", "O", "F", "Ne", "Na", "Mg", "Al",
	"Si", "P", "S", "Cl", "Ar", "K", "Ca", "Sc", "Ti", "V", "Cr", "Mn", "Fe",
	"Co", "Ni", "Cu", "Zn", "Ga", "Ge", "As", "Se", "Br", "Kr", "Rb", "Sr",
	"Y","Zr", "Nb", "Mo", "Tc", "Ru", "Rh", "Pd", "Ag", "Cd", "In", "Sn", "Sb",
	"Te", "I", "Xe", "Cs", "Ba", "Hf", "Ta", "W", "Re", "Os", "Ir", "Pt", "Au",
	"Hg", "Tl", "Pb", "Bi", "Po", "At", "Rn", "Fr", "Ra", "Rf", "Db", "Sg",
	"Bh", "Hs", "Mt", "Ds", "Rg", "Cn", "Fl", "Lv", "La", "Ce", "Pr", "Nd",
	"Pm", "Sm", "Eu", "Gd", "Tb", "Dy", "Ho", "Er", "Tm", "Yb", "Lu", "Ac",
	"Th", "Pa", "U", "Np", "Pu", "Am", "Cm", "Bk", "Cf", "Es", "Fm", "Md",
	"No", "Lr" };

char input[50002];
int alp_chk[50002];
int one[26];
int two[26][26];

void conversion(void)
{
	for (int i = 0; i < sizeof(chem) / 3; i++)
	{
		if (!chem[i][1])
			one[chem[i][0] - 'A'] = 1;
		else
			two[chem[i][0] - 'A'][chem[i][1] - 'a'] = 1;
	}
}

int main(void)
{
	FILE* pf;
	freopen_s(&pf, "input.txt", "r", stdin);
	int T;

	conversion();

	scanf_s("%d", &T);

	for (int t = 1; t <= T; t++)
	{
		int i;

		scanf_s("%s", input + 1, sizeof(input) + 1);

		alp_chk[0] = 1;

		for (i = 1; i <= strlen(input + 1); i++)
		{
			alp_chk[i] = 0;

			if (one[input[i] - 'a'])
				alp_chk[i] |= alp_chk[i - 1];

			if (two[input[i - 1] - 'a'][input[i] - 'a'])
				alp_chk[i] |= alp_chk[i - 2];
		}

		printf("Case #%d\n%s\n", t, alp_chk[i - 1] ? "YES" : "NO");
	}

	return 0;
}

//test case

