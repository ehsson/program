#include <stdio.h>
#include <math.h>

int main(void)
{
	int T, A, B, D;
	int minute;
	FILE* pf;
	freopen_s(&pf, "input.txt", "r", stdin);

	scanf_s("%d", &T);

	for (int t = 1; t <= T; t++)
	{
		scanf_s("%d %d %d", &A, &B, &D);

		minute = ceil(((double)D - (double)A) / ((double)A - (double)B)) + 1;

		printf("Case #%d\n%d\n", t, minute);
	}

	return 0;
}

//test case

4
2 1 5
5 1 6
100 99 1000000000
3 1 6
