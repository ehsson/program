#include <stdio.h>

long long arr[100000];
long long gap[100000];

int M;

long long GCD(long long num1, long long num2)
{
	long long n1, n2, temp;

	if (num1 <= num2)
	{
		n1 = num2;
		n2 = num1;
	}
	else
	{
		n1 = num1;
		n2 = num2;
	}

	while (1)
	{
		temp = n1 % n2;

		if (temp == 0)
			return n2;

		n1 = n2;
		n2 = temp;
	}
}

int CD(long long num)
{
	int cnt = 0;

	for (int i = 1; i <= num; i++)
		if (num % i == 0)
			cnt++;

	return cnt;
}

int main(void)
{
	int T;
	int chk;
	long long gcd;

	scanf("%d", &T);

	for (int t = 1; t <= T; t++)
	{
		scanf("%d", &M);

		gcd = 1;
		chk = 0;

		for (int m = 0; m < M; m++)
			scanf("%lld", &arr[m]);

		for (int n = 1; n < M; n++)
		{
			gap[n - 1] = arr[n] - arr[n - 1];

			if (gap[n - 1] == 0)
			{
				gcd = 0;
				chk++;
			}
		}

		if (gcd == 0)
		{
			if (chk == M - 1)
				printf("Case #%d\n1\n", t);
			else
				printf("Case #%d\n0\n", t);

			continue;
		}

		gcd = gap[0];

		for (int i = 1; i < M - 1; i++)
			gcd = GCD(gcd, gap[i]);

		printf("Case #%d\n%d\n", t, CD(gcd));
	}

	return 0;
}

// Test Case

3
3 
1 3 5
3
5 5 5
4
1 9 13 17
