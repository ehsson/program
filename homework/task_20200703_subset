// 모든 부분집합 구하기 문제

#include <stdio.h>

#define ELEMENT_MAX     100             // 원소 갯수 최대 100개

int element_num;
int chk[ELEMENT_MAX];
int arr[ELEMENT_MAX];

void printSubset(int N) {

    static int count = 0;
    int i;

    count++;

    printf("%d : ", count);             // 생성되는 부분집합(배열)의 갯수 출력

    for (i = 0; i < N; i++) {           // 각 부분 배열의 원소 출력
        if (chk[i] == 1)                // chk[i]가 1이면 포함된 것이므로 출력.
            printf("%d ", arr[i]);
    }

    printf("\n");
}

void subset(int n) {

    if (n == element_num) {             // 상태트리의 가장 아래에 다다르면 출력
        printSubset(element_num);       // 부분 집합 출력 (L[i]가 1인 모든 요소 출력)
        return;
    }

    chk[n] = 1;                         // n번 요소 포함
    subset(n + 1);                      // 다음 요소 포함 여부 결정

    chk[n] = 0;                         // n번 요소 포함 안함.
    subset(n + 1);                      // 다음 요소 포함 여부 결정
}

int main(void) {

    scanf_s("%d", &element_num);

    for (int i = 0; i < element_num; i++)
        scanf_s("%d", &arr[i]);

    subset(0);                          // arr[0] 칸 부터 시작

    return 0;
}
