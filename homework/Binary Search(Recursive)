#include <stdio.h>

int BSearch(int arr[], int start, int end, int target)
{
	if(start > end)
		return -1;
	
	int mid = (int)((start + end) / 2);
	
	if(arr[mid] == target)
		return mid;
	else if(target < arr[mid]) {
		BSearch(arr, start, mid - 1, target);
	}
	else {
		BSearch(arr, mid + 1, end, target);
	}
}

int main(void)
{
	int idx = 0;
	int arr[10] = { 0 };
	
	for(int i = 0; i < 10; i++)
		scanf("%d", &arr[i]);
	
	idx = BSearch(arr, 0, sizeof(arr) / sizeof(int) - 1, 5);
	
	if(idx == -1)
		printf("탐색 실패\n");
	else
		printf("타겟 저장 인덱스는 %d\n", idx);
	
	return 0;
}
