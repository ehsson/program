
// 아빠 솔루션

#include <stdio.h>

int main(void)
{
	char input[11] = { 0 };
	int cnt[10] = { 0 };

	scanf("%s", &input);

	for (int i = 0; i < 11; i++) {
		if (input[i] == 0) break;
		cnt[input[i] - '0']++;
	}

	for (int i = 9; i >= 0; i--)
		for (int k = 0; k < cnt[i]; k++)
			printf("%d", i);

	return 0;
}


