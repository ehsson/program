#include <stdio.h>

int main(void)
{
	int num1, num2, num3, num4, num5, num6;
	
	scanf("%d %d", &num1, &num2);
	
	num3 = num1 * (num2 % 10);
	num4 = num1 * ((num2 / 10) % 10);
	num5 = num1 * (num2 / 100);
	
	printf("%d\n%d\n%d\n", num3, num4, num5);
	
	num6 = num5 * 100 + num4 * 10 + num3;
	
	printf("%d\n", num6);
	
	return 0;
}
