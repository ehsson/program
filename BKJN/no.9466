// time limit code

#include <iostream>
#include <cstring>
using namespace std;

int N;
bool temp;
int student[100001];
int visit[100001];
bool chk[100001];

void DFS(int cur, int cnt)
{
	if (chk[cur])
		return;

	if (visit[cur] != 0) {
		temp = true;
		chk[cur] = true;
		return;
	}

	visit[cur] = cnt;

	DFS(student[cur], cnt + 1);

	visit[cur] = 0;

	if (chk[cur])
		temp = false;

	if (temp)
		chk[cur] = true;
}

int main(void)
{
	FILE* pf;
	freopen_s(&pf, "input.txt", "r", stdin);
	int T;
	cin >> T;

	for (int t = 1; t <= T; t++) {
		cin >> N;
		memset(chk, false, sizeof(chk));

		for (int n = 1; n <= N; n++) {
			cin >> student[n];

			if (student[n] == n)
				chk[n] = true;
		}

		for (int n = 1; n <= N; n++) {
			if (!chk[n]) {
				temp = false;
				DFS(n, 1);
			}
		}

		int cnt = 0;

		for (int n = 1; n <= N; n++)
			if (!chk[n])
				cnt++;

		cout << cnt << "\n";
	}

	return 0;
}
