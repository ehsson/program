// time limit code

#include <iostream>
using namespace std;

int arr[15][15];
int N, answer = 0;

void DFS(int cnt, int pos)
{
	if (pos == N * N) {
		if (cnt == N)
			answer++;

		return;
	}

	int y = pos / N;
	int x = pos % N;

	if (!arr[y][x]) {
		for (int i = 0; i < N; i++) {
			arr[y][i]++;
			arr[i][x]++;
			if (y >= i && x >= i)
				arr[y - i][x - i]++;
			if (y >= i && x + i < N)
				arr[y - i][x + i]++;
			if (x >= i && y + i < N)
				arr[y + i][x - i]++;
			if (y + i < N && x + i < N)
				arr[y + i][x + i]++;
		}

		DFS(cnt + 1, pos + 1);

		for (int i = 0; i < N; i++) {
			arr[y][i]--;
			arr[i][x]--;
			if (y >= i && x >= i)
				arr[y - i][x - i]--;
			if (y >= i && x + i < N)
				arr[y - i][x + i]--;
			if (x >= i && y + i < N)
				arr[y + i][x - i]--;
			if(y + i < N && x + i < N)
				arr[y + i][x + i]--;
		}
	}

	DFS(cnt, pos + 1);
}

int main(void)
{
	FILE* pf;
	freopen_s(&pf, "input.txt", "r", stdin);
	cin >> N;

	DFS(0, 0);

	cout << answer << "\n";

	return 0;
}
