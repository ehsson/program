// memory limit code

#include <iostream>
#include <queue>
using namespace std;

#define MAX 500000

int N, K;

queue<pair<pair<int, int>, int>> q; // {{cur, cnt}, k의 위치}

int main(void)
{
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	cin >> N >> K;

	int answer = -1;

	q.push({ { N, 0 }, K });

	while(!q.empty()) {
		int cur = q.front().first.first;
		int cnt = q.front().first.second;
		int k_pos = q.front().second;
		q.pop();

		if (cnt > MAX)
			break;

		if (cur == k_pos) {
			answer = cnt;
			break;
		}

		if (cur + 1 <= MAX && k_pos + (cnt + 1) <= MAX) {
			q.push({ { cur + 1, cnt + 1 }, k_pos + (cnt + 1) });
		}
		
		if (cur - 1 >= 0 && k_pos + (cnt + 1) <= MAX) {
			q.push({ { cur - 1, cnt + 1 }, k_pos + (cnt + 1) });
		}

		if (cur * 2 <= MAX && k_pos + (cnt + 1) <= MAX) {
			q.push({ { cur * 2, cnt + 1 }, k_pos + (cnt + 1) });
		}
	}

	cout << answer << "\n";

	return 0;
}
