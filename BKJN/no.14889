#include <iostream>
using namespace std;

int arr[21][21];
int chk[21];
int N, _min = 0b1111111111111111111111111111111;

void subset(int n)
{
	if (n == N + 1) {
		int cnt = 0;

		for (int i = 1; i <= N; i++)
			if (chk[i])
				cnt++;

		if (cnt == N / 2) {
			int sum1 = 0, sum2 = 0;

			for (int i = 1; i <= N; i++) {
				for (int j = 1; j <= N; j++) {
					if (i == j)
						continue;

					if (chk[i] == chk[j]) {
						if (chk[i])
							sum1 += arr[i][j];
						else
							sum2 += arr[i][j];
					}
				}
			}

			_min = min(_min, abs(sum1 - sum2));
		}

		return;
	}

	chk[n] = 1;
	subset(n + 1);

	chk[n] = 0;
	subset(n + 1);
}

int main(void)
{
	FILE* pf;
	freopen_s(&pf, "input.txt", "r", stdin);
	cin >> N;

	for (int y = 1; y <= N; y++)
		for (int x = 1; x <= N; x++)
			cin >> arr[y][x];

	subset(1);

	cout << _min << "\n";

	return 0;
}
