#include <iostream>
#include <string>
using namespace std;

int N, K;
string num[16];
int cache[51];
int cache_str[16];
int dp[32768][100];

long long GCD(long long a, long long b)
{
	while (a != 0) {
		long long rem = b % a;
		b = a;
		a = rem;
	}

	return b;
}

int get_div(string s)
{
	int div = 0;

	for (int i = 0; i < s.length(); i++) {
		div *= 10;
		div += s[i] - '0';
		div %= K;
	}

	return div;
}

int main(void)
{
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	cin >> N;

	for (int n = 0; n < N; n++) // 0000001, 0000010, 0000100, 0001000 ... 순으로
		cin >> num[n];

	cin >> K;

	cache[0] = 1 % K;

	for (int i = 1; i < 51; i++)
		cache[i] = (cache[i - 1] * 10) % K;

	for (int i = 0; i < N; i++)
		cache_str[i] = get_div(num[i]);

	dp[0][0] = 1;

	for (int cur = 0; cur < (1 << N); cur++) {
		for (int i = 0; i < N; i++) {
			if ((cur & (1 << i)) == 0) {
				int next_bit = cur | (1 << i);

				for (int j = 0; j < K; j++) {
					int next_k = ((j * cache[num[i].length()]) % K + cache_str[i]) % K;

					dp[next_bit][next_k] += dp[cur][j];
				}
			}
		}
	}

	long long cnt = dp[(1 << N) - 1][0];

	long long fac = 1;
	for (int i = 1; i <= N; i++)
		fac *= i;

	long long gcd = GCD(cnt, fac);

	cout << cnt / gcd << "/" << fac / gcd << "\n";

	return 0;
}
