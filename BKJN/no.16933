// fail code(memory limit)

#include <iostream>
#include <queue>
#include <string>
using namespace std;

#define MAX 2000000

int map[1001][1001];
bool visit[1001][1001][11];
int dy[4] = { 0, 1, 0, -1 };
int dx[4] = { 1, 0, -1, 0 };

int N, M, K;
int answer;

void BFS()
{
	queue<pair<pair<pair<int, int>, int>, int>> q;	// {{{y, x}, cnt}, k}
	q.push({ {{1, 1}, 1}, 0 });	// cnt가 홀수면 낮, 짝수면 밤
	visit[1][1][0] = true;

	while (!q.empty()) {
		int y = q.front().first.first.first;
		int x = q.front().first.first.second;
		int cnt = q.front().first.second;
		int k = q.front().second;
		q.pop();

		if (y == N && x == M) {
			answer = min(answer, cnt);
			return;
		}

		for (int i = 0; i < 4; i++) {
			int ny = y + dy[i];
			int nx = x + dx[i];

			if (ny >= 1 && ny <= N && nx >= 1 && nx <= M) {
				if (map[ny][nx] && k + 1 <= K && !visit[ny][nx][k + 1]) {	// 벽이 존재
					if (cnt % 2 == 1) {	// 낮
						visit[ny][nx][k + 1] = true;
						q.push({ {{ny, nx}, cnt + 1}, k + 1 });
					}
					else  // 밤
						q.push({ {{y, x}, cnt + 1}, k });
				}
				else if(!map[ny][nx]) {	// 벽이 존재하지 않음
					visit[ny][nx][k] = true;
					q.push({ {{ny, nx}, cnt + 1}, k });
				}
			}
		}
	}
}

int main(void)
{
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	cin >> N >> M >> K;

	for (int n = 1; n <= N; n++) {
		string s;
		cin >> s;

		for (int m = 0; m < M; m++)
			map[n][m + 1] = s[m] - '0';
	}

	answer = MAX;

	BFS();

	if (answer == MAX)
		cout << "-1\n";
	else
		cout << answer << "\n";

	return 0;
}
