#include <iostream>
using namespace std;

int main(void)
{
	int N;

	cin >> N;

	int answer = 0;

	for (int i = 1; i <= 1000000; i++) {
		int sum = i, temp = i;

		while (temp != 0) {
			sum += temp % 10;
			temp /= 10;
		}

		if (sum == N) {
			answer = i;
			break;
		}
	}

	cout << answer << endl;

	return 0;
}
